<%- include('partials/header') %>
<h1>Sign in</h1>
<form action="/auth/create" method="POST">
    <section>
        <label for="username">Username</label>
        <input id="username" name="username" type="text" autocomplete="username" value='jan@nowak.com' required autofocus>
    </section>
    <section>
        <label for="password">Password</label>
        <input id="password" name="password" type="password" autocomplete="password" value="admin" required>
    </section>
    <button type="submit">Sign in</button>
</form>
<%- include('partials/footer') %>
